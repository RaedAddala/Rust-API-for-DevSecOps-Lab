# Local development environment variables
# Copy to .env and adjust secrets before running `cargo run` or `docker compose up`

DATABASE_URL=postgres://library:library@localhost:5432/library
JWT_SECRET=local-dev-insecure-change-me
SERVER_PORT=8080
RUST_LOG=info,library_api=debug,sqlx=warn
UPLOAD_DIR=uploads
MAX_UPLOAD_BYTES=5242880

# used by sqlx for compile-time query checking
SQLX_DATABASE_URL=${DATABASE_URL}
